{
  "buildCommand": "cd ../.. && pnpm build --filter=docusaurus",
  "outputDirectory": "build",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": null,
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "trailingSlash": false
}